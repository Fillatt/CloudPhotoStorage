<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
			 xmlns:vw="using:CloudPhotoStorage.UI.Views"
			 xmlns:vm="using:CloudPhotoStorage.UI.ViewModels"
			 xmlns:aut_sections="using:CloudPhotoStorage.UI.Sections.Authorization"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
			 x:DataType="vm:AuthenticationViewModel"
             x:Class="CloudPhotoStorage.UI.Views.AuthenticationView">
	<StackPanel VerticalAlignment="Center"
				HorizontalAlignment="Center">
		<ContentControl Content="{Binding CurrentView}"/>
	</StackPanel>
</UserControl>
